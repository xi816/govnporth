include "./std/std.porth"

proc main in
  // push int
  10 20 30 print print print
  10 print 20 print 30 print
  
  "------------------------------\n" puts
  
  // push str
  "Hello, World\n" puts // ascii
  "Привет, Мир!\n" puts // utf-8
  "Hello, \"World\"\n" puts // string with double quotes
  
  "------------------------------\n" puts
  
  // Characters
  'E' print
  
  "------------------------------\n" puts
  
  // dup
  420 dup dup print print print
  69 dup print dup print print
  
  "------------------------------\n" puts
  
  // 2dup
  69 420 over over print print print print
  
  "------------------------------\n" puts
  
  // swap
  69 420 swap print print
  
  "------------------------------\n" puts
  
  // drop
  69 420 drop print
  
  "------------------------------\n" puts
  
  // over
  69 420 over print drop drop
  
  "------------------------------\n" puts
  
  // big number
  1234567890987654321 print
  
  "------------------------------\n" puts
  
  // rot
  1 2 3 print print print
  1 2 3 rot print print print
end
