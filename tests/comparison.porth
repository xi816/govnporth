include "std.porth"

proc main in
  // Equals
  69 420  = print
  69 69   = print
  
  "------------------------------\n" puts
  
  // Greater
  420 69  > print
  69 420  > print
  
  "------------------------------\n" puts
  
  // Less
  420 69  < print
  69 420  < print
  
  "------------------------------\n" puts
  
  // Greater Equals
  420 69 >= print
  69 420 >= print
  69 69  >= print
  
  "------------------------------\n" puts
  
  // Less Equals
  420 69 <= print
  69 420 <= print
  69 69  <= print
  
  "------------------------------\n" puts
  
  // Not Equals
  69 69  != print
  420 69 != print
end
